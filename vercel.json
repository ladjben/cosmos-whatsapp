{
  "version": 2,
  "builds": [
    {
      "src": "api/products.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "/api/products.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/api/products.js"
    }
  ]
}

